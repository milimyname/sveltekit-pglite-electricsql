{
	"name": "@mili-monorepo/sveltekit-pglite-electricsql",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"backend:up": "PROJECT_NAME=sveltekit-tanstack-electric pnpm run example-backend:up && pnpm run migrate",
		"backend:down": "PROJECT_NAME=sveltekit-tanstack-electric pnpm run example-backend:down",
		"backend:clear": "pnpm run  backend:down && pnpm run  backend:up",
		"example-backend:up": "dotenv -e .env.dev -- docker compose -f ../../.support/docker-compose.yml up -d",
		"example-backend:down": "dotenv  -e .env.dev -- docker compose -f ../../.support/docker-compose.yml down --volumes",
		"generate": "drizzle-kit generate",
		"migrate": "drizzle-kit migrate",
		"studio": "pnpx drizzle-kit studio"
	},
	"devDependencies": {
		"@faker-js/faker": "^9.0.3",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0-next.6",
		"@types/eslint": "^9.6.0",
		"@types/uuid": "^10.0.0",
		"autoprefixer": "^10.4.20",
		"dotenv": "^16.4.5",
		"dotenv-cli": "^7.4.2",
		"drizzle-kit": "^0.26.2",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"postcss": "^8.4.47",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^5.0.0-next.1",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.14",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"@electric-sql/client": "^0.6.3",
		"@electric-sql/pglite": "^0.2.12",
		"@electric-sql/pglite-sync": "^0.2.13",
		"@tanstack/svelte-query": "^5.59.13",
		"@tanstack/svelte-query-devtools": "^5.59.13",
		"bits-ui": "1.0.0-next.20",
		"drizzle-orm": "^0.35.1",
		"postgres": "^3.4.4",
		"uuid": "^10.0.0",
		"zod": "^3.23.8",
		"@repo/ui": "workspace:*"
	}
}
